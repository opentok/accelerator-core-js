{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "extends": "airbnb",
    "parser": "babel-eslint",
    "rules": {
        "no-confusing-arrow": 1,
        "no-unused-expressions": 1,
        "no-prototype-builtins": 1,
        "max-len": ["error", 120]
    },
    "globals": {
        "OT": true
    }
}
